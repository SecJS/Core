{
  "name": "@jlenon7/dedsec",
  "version": "1.1.8",
  "description": "",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "yarn lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "yarn lint && yarn clean && tsc",
    "build": "yarn compile",
    "prepublishOnly": "yarn build",
    "coverage": "nyc --reporter=lcov --reporter=text --clean node japaFile.js && http-server test/coverage/lcov-report",
    "ci-test": "nyc --reporter=text-summary --clean node japaFile.js",
    "lint": "eslint ./src --ext=.ts",
    "lint:fix": "eslint ./src --fix --ext=.ts",
    "dev": "tsc --watch",
    "commit": "git-cz",
    "release": "np",
    "version": "yarn build"
  },
  "dependencies": {},
  "devDependencies": {
    "@adonisjs/mrm-preset": "2.0.3",
    "@types/cls-hooked": "^4.3.0",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.154",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "cls-hooked": "^4.2.2",
    "commitizen": "^4.0.4",
    "cz-conventional-changelog": "^3.1.0",
    "del-cli": "^3.0.0",
    "doctoc": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "express": "^4.17.1",
    "http-server": "^0.12.3",
    "husky": "^4.2.5",
    "japa": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mrm": "^2.2.1",
    "np": "^6.2.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3",
    "uuidv4": "^6.1.1"
  },
  "author": "",
  "license": "Unlicense",
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "main": "build/",
  "types": "build/",
  "files": [
    "build",
    "build/Responses/index.d.ts",
    "build/Responses/index.js"
  ],
  "husky": {
    "hooks": {
      "pre-push": "yarn lint",
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  }
}
